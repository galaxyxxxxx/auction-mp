page{
  font-size: var(--font-size-S);
  height: 100%;
  
  background-color: #f9f9fa;
}

.container{
  height: calc(100vh);
  position: absolute;
  background-color: #f9f9fa;
  background-size: 100% 100%;
}

/* 顶部个人信息 高10%*/

.userinfo{
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #f0f0f1;
  width: calc(100vw);
  height: calc(6vh);
  padding: 0 3vw;
  box-sizing: border-box;
  background-color: var(--color-theme);
  background-clip: border-box;
}
.hello{
  line-height: 50rpx;
  font-size: 28rpx;
}
.today{
  font-size: 40rpx;
  line-height: 70rpx;
  font-weight: bold;
  color: #fff;
}
.avatar{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 20rpx 0;
  border: 1px solid rgb(99, 99, 99, 0.1);
}
.nickname{
  margin-left: 20rpx;
}

/* 主体 */
.main {
  width: 100%;
  top:  calc(15vh);
  height: calc(85vh);
  overflow: hidden;
}

/* 时间线 */
.timeline{
  border-top: 5vh solid var(--color-theme);
  box-sizing: border-box;
  width: calc(10vw);
  height: calc(85vh);

  float: left;
  display: flex;
  flex-direction: column;
  align-content: center;

  background-color: #f9f9fa;
  background-clip: content-box;
  color: rgb(99, 99, 99);
}
.hourpoint{
  height: calc(8vh);
  display: flex;
  justify-content: flex-end;
  align-items: center;
  opacity: 0.6;
}

/* 滚动区域 width90%*/
.scroll{
  display: flex;
  height: calc(90vh);
  width: calc(90vw);
  overflow: hidden;
  background-clip: content-box;
  background-color: #f9f9fa;
}
/* 一列 */
.day{
  /* flex: 1; */
  min-width: calc(30vw);
  height: calc(90vh);
  display: flex;
  flex-direction: column;
  background: url('data:image/svg+xml;base64,');
  background-position-y: calc(5vh);
  background-position-x: -5px;
  background-size: auto calc(80vh);
  background-repeat: no-repeat;
}
/* 日期 */
.date{
  height:calc(5vh);
  text-align: center;  
  font-size: 28rpx;
  color: #fff;
  line-height: calc(4vh);
  background-color: var(--color-theme);
}

/* 活动 */
.lab{
  position: absolute;
  display: flex;
  flex-direction: column;
  justify-content: center;

  width: calc(30vw - 50rpx);
  margin-left: 16rpx;
  padding: 0 18rpx;

  border: 2rpx solid #3d77ff34;
  background-color: var(--color-theme2);
  border-radius: 8rpx;

  color: var(--color-theme);
  line-height: 50rpx;
}

.lab:active{
  opacity: 0.6;
  transform: scale(1.02);
  border: 2rpx solid #3D78FF;
}

.titleMini{
  /* margin-top: 10rpx; */
  font-weight: bold;
  line-height: 30rpx;
  font-size: 24rpx;
}

.title{
  margin-top: 16rpx;
  font-weight: bold;
  line-height: 30rpx;
  font-size: 28rpx;
}

.time{
  font-size: var(--font-size-S);
}

.add{
  width: 100rpx;
  height: 100rpx;

  z-index: 1;
  position: fixed;
  right: 40rpx;
  bottom: 200rpx;

  background: url(data:image/svg+xml;base64,);

  background-size: 40% 40%;
  background-repeat: no-repeat;
  background-position: center;
  background-color: var(--color-theme);
  border-radius: 50%;
  box-shadow: 0 10rpx 15rpx var(--color-theme2);
}


.history{
  width: 100rpx;
  text-align: center;
  position: fixed;
  left: 0;
  top: 350rpx;
  color: #fff;
  /* background-color: green; */
  z-index: 2;
}

.order{
  width: calc(10vw);
  height: calc(5vh);

  z-index: 1;
  position: absolute;
  left: calc(1vw);
  top: calc(9.5vh);

  background: url(data:image/svg+xml;base64,);
  background-size: 62% 62%;
  background-repeat: no-repeat;
  background-position: center;
  /* background-color: #FFAD3E; */
}